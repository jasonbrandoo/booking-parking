{
  "extends": "airbnb",
  "rules": {
    "no-console": "off",
    "max-len": "off",
    "operator-linebreak": "off",
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "react/jsx-one-expression-per-line": [0],
    "react/destructuring-assignment": [0],
    "react/prop-types": [0],
    "jsx-a11y/label-has-for": [0]
  },
  "parser": "babel-eslint"
}
